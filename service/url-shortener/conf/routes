# REST API

POST    /url-shortener/v1/url   controllers.RestController.insert
GET     /url-shortener/v1/url   controllers.RestController.lookup(shortUrl)

GET     /$id<[_a-zA-Z0-9-]{2,}> controllers.RestController.redirect(id)
PATCH   /$id<[_a-zA-Z0-9-]{2,}> controllers.RestController.redirect(id)
POST    /$id<[_a-zA-Z0-9-]{2,}> controllers.RestController.redirect(id)
PUT     /$id<[_a-zA-Z0-9-]{2,}> controllers.RestController.redirect(id)
DELETE  /$id<[_a-zA-Z0-9-]{2,}> controllers.RestController.redirect(id)
HEAD    /$id<[_a-zA-Z0-9-]{2,}> controllers.RestController.redirect(id)

GET     /assets/*file           controllers.Assets.at(path="/public", file)

# Sample routes generated by playframework
GET     /                       controllers.HomeController.index
->      /v1/posts               v1.post.PostRouter
